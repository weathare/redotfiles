cd         # enhancd::run
mcd        # mkdir -p $1 && cd $1
md         # mkdir -p
rm         # rm -i
grep       # grep --color

# git
fa         # git alias | fzf
fad        # git add | fzf
fbr        # git branch --all | grep -v HEAD | fzf | git checkout  $target
fco        # git tag | git branch --all | fzf | git checkout $target
fshow      # git log master..$current_branch | fzf

# control go(git) package
fcd        # gch -l | fzf
frepo      # ghq lis | fzf && cd $GHQ_ROOT/$target
ghq-update # ghq list | sed | xargs ghq get -u
gti        # alias git

# apt
givme      # sudo apt-get install
disposeof  # sudo apt-get remove
show       # apt-cache search $1

# directory
l          # ls -CF
la         # ls -A
ll         # ls -lFh
lla        # ls ls -alh
ls         # ls --color=auto
cls        # clear && ls -F

# make
m          # make
make       # clear && make
smi        # sudo make install

# system
off        # sudo shutdown -r now
reboot     # sudo shutdown -r now
reload     # source ~/.bashrc
h          # history
keys       # ssh-add -l
date +%Y%m%d

# searach
lsof -i:80,3000,8000,8080,5432,6379 | grep LISTEN
lsof -c ruby

